{
	"name": "Trello Labelize",
	"version": "0.1",
	"manifest_version" : 2,
	"description": "Chrome extension to control the display of labels on Trello",
	"content_scripts": [
	{
		"matches": ["https://trello.com/*"],
		"js": ["lib/jquery-1.8.3.min.js", "lib/mutation_summary.js", "trelloLabelize.js"]
	}
	],
	"icons": {
		"16": "icons/16.png",
		"48": "icons/48.png",
		"128": "icons/128.png"
	}
}
